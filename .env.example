# LaunchDarkly SDK Configuration
# Copy this file to .env and fill in your actual values

# Required: Your LaunchDarkly SDK key
LAUNCHDARKLY_SDK_KEY=your-sdk-key-here

# Optional: Set to any value when running in CI environment
# CI=true

# Required: AWS credentials for Amazon Bedrock (Nova models)
AWS_ACCESS_KEY_ID=your-aws-access-key-here
AWS_SECRET_ACCESS_KEY=your-aws-secret-key-here
AWS_REGION=us-east-1

# Optional: LaunchDarkly Client-Side ID for JavaScript SDK
# This is different from the SDK key - get it from your LaunchDarkly project settings
# Client-side IDs are safe to expose in browser code
LAUNCHDARKLY_CLIENT_SIDE_ID=your-client-side-id-here

# Optional: Application metadata for observability
APPLICATION_ID=hello-dotnet-server
APPLICATION_VERSION=1.0.0
